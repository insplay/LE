          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "& when (@showImage)": {
              ".card-img-wrap": {
                "padding-bottom": "2rem"
              },
              ".card-img": {
                "margin": "0 auto",
                "overflow": "hidden",
                "width": "~\"@{imageSize}px\"",
                "height": "~\"@{imageSize}px\"",
                "amp-img img": {
                  "border-radius": "50%"
                },
                "@media (max-width: 767px)": {
                  "width": "150px",
                  "height": "150px"
                }
              }
            },
            ".mbr-section-title": {
              "padding-bottom": "1rem"
            },
            ".icon-wrap": {
              "display": "inline-block",
              "width": "~\"@{iconsSize}rem\"",
              "height": "~\"@{iconsSize}rem\"",
              "margin": ".7rem"
            },
            ".amp-iconfont": {
              "color": "@iconsColor",
              "font-size": "~\"@{iconsSize}rem\""
            },
            ".circle": {
              "display": "inline-block",
              "background": "@iconsBg",
